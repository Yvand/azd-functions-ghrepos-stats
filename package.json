{
  "name": "releases-statistics",
  "version": "",
  "scripts": {
    "build": "tsc",
    "build:production": "npm run prestart && npm prune --production",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start",
    "start": "npm-run-all --parallel start:host watch",
    "test": "echo \"No tests yet...\"",
    "codegen:generate": "apollo codegen:generate --target typescript --localSchemaFile=./schema.graphql --tsFileExtension=d.ts --outputFlat=app/graphql_types --tagName=gql --excludes=node-modules/* --includes=app/**/*.ts --endpoint https://api.github.com/graphql --header \"Authorization: Bearer $npm_config_GITHUB_TOKEN\""
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^3.0.0",
    "@types/node": "^16.11.11",
    "apollo": "^2.33.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@azure/cosmos": "^3.14.1",
    "apollo-boost": "^0.4.9",
    "cross-fetch": "^3.1.4",
    "graphql": "^15.7.2"
  }
}
