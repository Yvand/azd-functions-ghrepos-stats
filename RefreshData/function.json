{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */3 * * * *",
      "runOnStartup": true
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "ldapcpStats-db",
      "collectionName": "ldapcpStats-coll",
      "createIfNotExists": "false",
      "connectionStringSetting": "COSMOS_CONNECTION",
      "partitionKey": "/DateStatCreatedYYYYMM"
    }
  ],
  "scriptFile": "../dist/RefreshData/index.js"
}
